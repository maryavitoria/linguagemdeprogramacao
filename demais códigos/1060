#include <stdio.h>
int main(){
    double a, b, c, d, e, f;
    int soma;
    scanf("%lf%lf%lf%lf%lf %lf", &a, &b, &c, &d, &e, &f);
    if (a>0){
        a=1;
    }
    else if (a<0){
        a=0;
    }
    if (b>0 && b!=0){
        b=1;
    }
        else if (b<0 && b!=0){
        b=0;
    }
    if (c>0 && c!=0){
        c=1;
    }
        else if (c<0 && c!=0){
        c=0;
    }
    if (d>0 && d!=0){
        d=1;
    }
        else if (d<0 && d!=0){
        d=0;
    }
    if (e>0 && e!=0){
        e=1;
    }
        else if (e<0 && e!=0){
        e=0;
    }
    if (f>0 && f!=0){
        f=1;
    }
        else if (f<0 && f!=0){
        f=0;
    }

    soma = a+b+c+d+e+f;

printf("%d valores positivos\n", soma);

return 0;
}
