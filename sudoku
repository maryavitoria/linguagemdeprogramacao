#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,a,k=0,n,sl=0,sc=0,x=0,cont=0,sq=0,l=0,v[10];
    int m[9][9];
    scanf("%d",&n);
    while(cont<n)
    {
        for(i=0; i<=9; i++)
        {
            v[i]=0;
        }
        for(i=0; i<9; i++) //lendo o sudoku
        {
            for(a=0; a<9; a++)
            {
                scanf("%d",&m[i][a]);
            }
        }
        //verificando linhas e colunas
        for(i=0; i<9; i++)
        {
            sl=0;
            sc=0;
            for(a=0; a<9; a++)
            {
                sl+=m[i][a];
                sc+=m[a][i];
                v[m[i][a]]++;

            }
            if(sl!=45 || sc!=45)
            {
                i=9;
                x=1;
            }
        }
        //verificar quadrante
        while(l<=6)
        {
            k=0;
            while(k<=6)
            {
                sq=0;
                for(i=l; i<3+l; i++)
                {

                    for(a=k; a<3+k; a++)
                    {
                        sq+=m[i][a];
                    }
                }
                if(sq!=45)
                {
                    l=6;
                    x=1;
                }
                k=k+3;
            }
            l=l+3;
        }

        //verifica repetiÃ§ao
        for(i=1; i<=9; i++)
        {
            if(v[i]!=9)
            {
                x=1;
                i=9;
            }
        }
        if(x==0)
        {
            printf("Instancia %d\n",cont+1);
            printf("SIM\n\n");
        }
        else
        {
            printf("Instancia %d\n",cont+1);
            printf("NAO\n");
            printf("\n"); //linha
        }

        cont++;
    }
    return 0;
}
